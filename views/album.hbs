<button class="back" onclick=window.history.back()>Return to Albums</button>
<div class="album-tracklist">
<ul>
  {{#each trackResults}}
  <li>
    <h3>{{ name }}</h3>
    <figure>
    <audio
        controls
        src="{{ preview_url }}">
            Your browser does not support the
            <code>audio</code> element.
    </audio>
</figure>

  </li>
  {{/each}}
</ul>
</div>